<!DOCTYPE html>
<html lang="ja">
  <head>
    <meta charset="utf-8">
    <title>Node.js Demo</title>
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/mousetracer.css" rel="stylesheet">
  </head>

  <body>

    <div class="navbar navbar-inverse navbar-fixed-top" role="navigation">
      <div class="container">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a class="navbar-brand" href="/">Node.js Demo</a>
        </div>
        <div class="collapse navbar-collapse">
          <ul class="nav navbar-nav">
            <li class="active"><a href="/mousetracer">Mouse Tracer</a></li>
            <li><a href="/chat">Chat</a></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </div>

    <div id="contents" class="container">

      <div class="row">
        <ul id="visitor-list" class="col-md-2 col-md-offset-10 col-sm-2 col-sm-offset-10 col-xs-2 col-xs-offset-10 list-group">
        </ul>
      </div>

    </div><!-- /.container -->
    <div id="svg"></div>

    <script type="text/template" id="visitor">
        <li id="id-{{= id }}" class="list-group-item"><span class="badge pull-right" style="background-color: {{= color }};">{{= count }}</span>{{= id }}</li>
    </script>

    <script src="/js/jquery-2.1.0.min.js"></script>
    <script src="/js/bootstrap.min.js"></script>
    <script src="/socket.io/socket.io.js"></script>
    <script src="/js/raphael-min.js"></script>
    <script src="/js/ejs.js"></script>
    <script src="/js/mousetracer.js"></script>
  </body>
</html>
